doctype html
html(lang="en")
  head
    meta(name="viewport", content="width=device-width, initial-scale=1")
    title= config.title

    each css in cssFiles
      link(rel="stylesheet", type="text/css", href="#{config.staticRoot}#{css}")
    each js in jsFiles
      script(type="text/javascript", src="#{config.staticRoot}#{js}")
  body
    #w-data-root= config.dataRoot
    #w-static-root= config.staticRoot
